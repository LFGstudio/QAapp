{
  "version": 2,
  "builds": [
    {
      "src": "api/working-with-images.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.{js,css,html,svg,png,webmanifest,json}",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/working-with-images.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}