{
  "version": 2,
  "builds": [
    {
      "src": "api/working-with-images.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/working-with-images.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}